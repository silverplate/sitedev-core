<?xml version="1.0"?>
<!DOCTYPE form SYSTEM "../../../core/src/Ext/Xml/entities.dtd">

<form>
    <element name="name" type="name" is-required="true">
        <label>Имя</label>
    </element>

    <element name="email" type="email" is-required="true">
        <label>Электропочта</label>
    </element>

    <element name="passwd" type="password">
        <label>Пароль</label>
    </element>

    <element name="phone" type="word">
    <!-- <element name="phone" rem-type="phone" type="string"> -->
        <label>Телефон</label>
    </element>

    <element name="status_id" type="boolean">
        <label>Доступ разрешен</label>
    </element>
</form>
